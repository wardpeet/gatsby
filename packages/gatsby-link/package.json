{
  "name": "gatsby-link",
  "description": "An enhanced Link component for Gatsby sites with support for resource prefetching",
  "version": "3.8.0-next.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "@types/reach__router": "^1.3.7",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@testing-library/react": "^9.5.0",
    "babel-preset-gatsby-package": "^1.8.0-next.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "@gatsbyjs/reach-router": "^1.3.5",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-link#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": "./dist/index.modern.js",
  "module": "./dist/index.module.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-link"
  },
  "scripts": {
    "build:microbundle": "microbundle build -i src/index.js -f cjs,modern,esm --jsx createElement --no-compress",
    "build": "babel src --out-dir . --ignore \"**/__tests__\" --extensions \".tsx,.js\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\" --extensions \".tsx,.js\"",
    "watch:microbundle": "microbundle build -i src/index.js -f cjs,modern,esm --jsx createElement --no-compress"
  },
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12.13.0"
  }
}